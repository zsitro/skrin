.l-404--animated
	display none

@-webkit-keyframes swing
	0%
		-webkit-transform rotateZ(-3deg)
	50%
		-webkit-transform rotateZ(3deg)
	100%
		-webkit-transform rotateZ(-3deg)


@-moz-keyframes swing
	0%
		-moz-transform rotateZ(-3deg)
	50%
		-moz-transform rotateZ(3deg)
	100%
		-moz-transform rotateZ(-3deg)


@-ms-keyframes swing
	0%
		-ms-transform rotateZ(-3deg)
	50%
		-ms-transform rotateZ(3deg)
	100%
		-ms-transform rotateZ(-3deg)		

