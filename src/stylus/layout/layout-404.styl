.csstransforms
	.l-404--static
		display none
	.l-404--animated
		display block


		.l-404__animation
			position relative
			overflow hidden

			.l-404__image
				position absolute
				top 0
				left 5%
				padding 0
				//margin 0
				outline 0px solid
				display block

			.l-404__image--0
				left 6%
				-webkit-transform-origin 49% 40%
				//margin-top -56%
				//-webkit-transform translateY(-100%)
				animation-duration 5s
				animation-name swing
				animation-iteration-count infinite
				animation-timing-function ease-in-out





















